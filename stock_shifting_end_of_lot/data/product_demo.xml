<?xml version="1.0"?>
<odoo>

    <record id="product_4_end_lot1" model="product.product">
        <field name="name">Product 1 with lot</field>
        <field name="categ_id" ref="product.product_category_all"/>
        <field name="type">product</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
        <field name="default_code">SSRQ1</field>
        <field name="tracking">lot</field>
        <field name="company_id" eval="0"/>
    </record>
    <record id="product_4_end_lot2" model="product.product">
