    <template id="report_timesheet" inherit_id="hr_timesheet.report_timesheet">
        <xpath expr="(//th)[count(//td/span[@t-field='l.user_id.partner_id.name']/../preceding-sibling::*)+1]" position="after">
            <th><span>Role</span></th>
        </xpath>
        <xpath expr="//td/span[@t-field='l.user_id.partner_id.name']/.." position="after">
            <td>
                <span t-field="l.role_id.name"/>
            </td>
        </xpath>
        <xpath expr="//tbody/tr[last()]/td[2]" position="after">
            <td />
        </xpath>
    </template>

</odoo>
