<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Set field service location routes from the contact -->
    <record id="fsm_partner_fields" model="ir.ui.view">
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="fieldservice.fsm_partner_fields" />
        <field name="arch" type="xml">
            <field name="service_location_id" position="after">
                <field name="fsm_route_id" />
            </field>
        </field>
    </record>
</odoo>
