<odoo>
    <record id="chained_swapper_wizard_view_form" model="ir.ui.view">
        <field name="name">chained.swapper.wizard.form</field>
        <field name="model">chained.swapper.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group col="4" name="swap_field_group"/>
                <footer>
                    <button name="change_action"
                            string="Change"
                            type="object"
                            class="btn-primary"/>
                    <button string="Close"
                            class="btn-default"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
